#include <stdio.h>

    int main (void){
        int matriz[4][3]={{1,2,4},{2,3,4},{5,6,7},{8,9,10}};
        int matriz2[4][3]={0};
        for (int i=0; i<3; i++){
            for(int j=0;j<4; j++){
                printf(" %d ", matriz[i][j]);
            }
            printf(" \n ");
        }     
        return 0;
}
